{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gs-test-adf"
		},
		"AzureDataLakeStorageSource_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageSource'"
		},
		"SnowflakeTableTarget_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'SnowflakeTableTarget'"
		},
		"AzureDataLakeStorageSource_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gsadlsaccount.dfs.core.windows.net/"
		},
		"SnowflakeTableTarget_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SALES_MART"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "gs-test-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageSource_accountKey": {
						"value": "[parameters('AzureDataLakeStorageSource_accountKey')]"
					},
					"SnowflakeTableTarget_password": {
						"value": "[parameters('SnowflakeTableTarget_password')]"
					},
					"AzureDataLakeStorageSource_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageSource_properties_typeProperties_url')]"
					},
					"SnowflakeTableTarget_properties_typeProperties_database": {
						"value": "[parameters('SnowflakeTableTarget_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}